<h1>People</h1>
<table class="table table-striped">
  <tr>
    <th>Name</th>
    <th>National Insurance Number</th>
    <th></th>
  </tr>
  <%@people.each do |person|%>
    <tr>
      <td><%=link_to person.name, patch_assign_house_path(@house, person), method: :patch %></td>
      <td><%=person.nin%></td>
    </tr>
  <%end%>
</table>
